// prisma/schema.prisma
datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL") 
}

generator client {
  provider = "prisma-client-js"
}

// Define User
model User {
  id       Int    @id @default(autoincrement())
  email    String @unique @db.VarChar(20) 
  password String @db.VarChar(70) 
  nickname String @unique @db.VarChar(10) 
}